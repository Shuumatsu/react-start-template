{
  "presets": [
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 Chrome versions"
              ]
            },
            "spec": true,
            "modules": false
          }
        ]
      ],
      "plugins": [
        "transform-react-jsx-source"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "ie >= 10"
              ]
            },
            "spec": true,
            "modules": false
          }
        ]
      ],
      "plugins": [
        "transform-class-properties",
        [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
          }
        ]
      ]
    }
  }
}